@{
    ViewData["Title"] = "Import Hand History";
}

<h1>Import Hand History Files</h1>

<form method="post" enctype="multipart/form-data" asp-controller="HandHistory" asp-action="Import">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload one or more files using this form:</p>
            <input type="file" name="files" multiple>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input class="btn btn-primary" type="submit" value="Upload">
        </div>
    </div>
</form>
